
public class Main {

	public static void main(String[]args) {
		Cat cat = new Cat();
		cat.setPetName("Luna");
		
		Dog dog = new Dog();
		dog.setPetName("Abby");
		
		Goldfish fish = new Goldfish();
		fish.setPetName("Goldy");
		
		cat.makeNoise();
		dog.makeNoise();
		fish.makeNoise();
		
		cat.stroke();
		dog.stroke();
		
		PetShop shop = new PetShop();
		shop.buyCat();
		shop.buyCat();
		
	}
}


public class Main {

	public static void main(String[]args) {
		Cat cat = new Cat();
		cat.setPetName("Luna");
		
		Dog dog = new Dog();
		dog.setPetName("Abby");
		
		Goldfish fish = new Goldfish();
		fish.setPetName("Goldy");
		
		cat.makeNoise();
		dog.makeNoise();
		fish.makeNoise();
		
		cat.stroke();
		dog.stroke();
		
		PetShop shop = new PetShop();
		StrokeATron t1 = new StrokeATron();
		
		t1.strokeAll(shop.getAllStrokeablePets());
		
		shop.buyPetByCost(60);
		shop.buyCat();
		shop.buyCat();
		shop.buyDog();
		shop.buyCat();
		shop.buyGoldfish();
		shop.buyDog();
		shop.buyDog();
		shop.buyDog();
		shop.buyGoldfish();
		shop.buyGoldfish();
		shop.buyGoldfish();
		shop.buyGoldfish();
		shop.buyPetByCost(50);
		
		
		
	}
}
